enum Gender {
    X
    MAN
    WOMEN
}

type User {
    id: ID!
    username: String
    firstName: String
    lastName: String
    gender: Gender
    locale: Locale
    address: Address
    contact: Contact
    bio: Bio
}

input UserUpdateInput {
    firstName: String
    lastName: String
    username: String
    gender: Gender
    locale: String
    address: AddressInput
    contact: ContactInput
    bio: BioInput
}

extend type Query {
    user: User!
}